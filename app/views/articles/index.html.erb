<h1>Articles</h1>
<%= link_to 'Add new Articles', new_article_path() %> 
<br><br><br>
<% @articles.each do |article| %>
  <p><%= article.title %></p>
  <p><%= article.like %></p>
<p> <%= link_to 'Edit', edit_article_path(article) %>  | 
    <%= link_to 'Read', article_path(article) %>    |
    <%= link_to 'Delete', article_path(article), method: :delete, 
          data: {confirm: 'Are you sure want to delete this article '} %> </p>
  <hr>
<% end %>